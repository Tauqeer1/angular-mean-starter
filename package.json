{
  "name": "angular-mean-starter",
  "version": "1.1.3",
  "description": "Angular MEAN starter - MongoDB + ExpressJS + Angular + NodeJS",
  "homepage": "https://github.com/jussikinnula/angular-mean-starter",
  "repository": "https://github.com/jussikinnula/angular-mean-starter",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "build:watch": "webpack --config config/webpack.development.ts --watch",
    "build": "npm run build:prod",
    "clean": "rimraf target",
    "predev": "npm run clean && npm run build:dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run build:watch\"",
    "postinstall": "npm run build",
    "server:prod": "node target/server.js",
    "server:dev": "nodemon target/server.js --ignore target/assets",
    "server": "npm run server:prod",
    "start": "npm run server"
  },
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/mime": "^1.3.1",
    "@types/mongodb": "^2.2.7",
    "@types/mongoose": "^4.7.19",
    "@types/node": "^8.0.14",
    "@types/serve-static": "^1.7.31",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "email-validator": "^1.0.7",
    "exports-loader": "^0.6.4",
    "express": "^4.15.3",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "http-auth": "^3.2.3",
    "js.clone": "^0.0.3",
    "moment": "^2.18.1",
    "mongoose": "^4.11.3",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "serve-static": "^1.12.3",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.3.1",
    "ts-node": "~3.2.1",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@types/webpack": "^3.0.4",
    "concurrently": "^3.5.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "nodemon": "~1.11.0",
    "webpack-dev-server": "~2.5.1"
  }
}
